{
  "name": "Monkey Patching Example Scope",
  "version": "0.1.0",
  "manifest_version": 2,
  "permissions": ["tabs", "activeTab", "*://*/*"],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "scope-for-chrome-icon-16x16.png",
      "32": "scope-for-chrome-icon-32x32.png",
      "48": "scope-for-chrome-icon-48x48.png",
      "128": "scope-for-chrome-icon-128x128.png"
    }
  },
  "background": {
    "scripts": ["background.js"]
  },
  "icons": {
    "16": "scope-for-chrome-icon-16x16.png",
    "32": "scope-for-chrome-icon-32x32.png",
    "48": "scope-for-chrome-icon-48x48.png",
    "128": "scope-for-chrome-icon-128x128.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
